program Project1;

uses
  Vcl.Forms,
  uCadastro in 'uCadastro.pas' {frCadastroAluno},
  uEstagioDesejado in '..\aula2\uEstagioDesejado.pas' {frEstagiosInf},
  uEstagioDesejadoQui in '..\aula2\uEstagioDesejadoQui.pas' {frEstagioQui};

