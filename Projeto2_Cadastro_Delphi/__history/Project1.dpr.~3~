program Project1;

uses
  Vcl.Forms,
  uCadastro in 'uCadastro.pas' {frPessoa},
  uMenu in 'uMenu.pas' {frMenu};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TfrPessoa, frPessoa);
  Application.CreateForm(TfrMenu, frMenu);
  Application.Run;
end.
